pipeline {
  agent any
  stages {
    stage('阶段 1-1') {
      steps {
        checkout([
          $class: 'GitSCM',
          branches: [[name: env.GIT_BUILD_REF]],
          userRemoteConfigs: [[
            url: env.GIT_REPO_URL,
            credentialsId: env.CREDENTIALS_ID
          ]]])
          sh 'echo hello'
        }
      }
    }
  }
